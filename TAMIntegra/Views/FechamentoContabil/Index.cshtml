                $('#observacao').val("");
                $('#gridCambio tbody').empty();
                $('#erroFechamento').hide();
                $('#erroDtContabil').hide();
                $('#erroSituacao').hide();
            });

            $('#modalUpdateSitFechado').on('hidden.bs.modal', function (e) {
                $('#modal_dt_contabil').val("");
                $('#modal_dt_abertura').text("");
                $('#modal_dt_fechamento').text("");
                $('#modal_responsavel_abertura').text("");
                $('#modal_responsavel_fechamento').text("");
                $('#observacao').val("");
                $('#gridCambio tbody').empty();
                $('#erroFechamento').hide();
                $('#erroDtContabil').hide();
                $('#erroSituacao').hide();
            })


            function Salvar() {
                let fechamento = $('#m_fechamento').val();
                let dt_contabil = $('#modal_dt_contabil').val();
                let dt_abertura = $('#modal_dt_abertura').text();
                let dt_fechamento = $('#modal_dt_fechamento').text();
                let resp_abertura = $('#modal_responsavel_abertura').text();
                let resp_fechamento = $('#modal_responsavel_fechamento').text();
                let id_fechamento = $('#modal_id_fechamento').val();
                let situacao = $('#m_situacao option:selected').val();
                let obs = $('#observacao').val();
                let id_integracao = $("input[name='ckCambio']:checked").val();

                if ((fechamento != null & fechamento != "") & (dt_contabil != null & dt_contabil != "") & situacao != "-- Selecione --")
                {
                    $.ajax({
                        url: '@Url.Action("Salvar", "FechamentoContabil")',
                        traditional: true,
                        contentType: 'json',
                        data: { id_fechamento: id_fechamento, id_integracao: id_integracao, fechamento: fechamento, dt_contabil: dt_contabil, situacao: situacao, observacao: obs },
                        success: function (response) {
                            if (response.mensagem != "") {
                                alert(response.mensagem)
                            }
                            else {
                                $('#myForm').submit();
                            }
                        }
                    });
                }
                else {
                    if (fechamento == "") {
                        $('#m_fechamento').addClass("erroInput");
                        $('#erroFechamento').addClass("erroDiv");
                        $('#erroFechamento').show();
                    }

                    if (dt_contabil == "" || dt_contabil == null) {
                        $('#modal_dt_contabil').addClass("erroInput");
                        $('#erroDtContabil').addClass("erroDiv");
                        $('#erroDtContabil').show();
                    }

                    if (situacao == "-- Selecione --") {
                        $('#m_situacao').addClass("erroInput");
                        $('#erroSituacao').addClass("erroDiv");
                        $('#erroSituacao').show();
                    }
                }
            }

            $('#m_fechamento').keyup(function () {
                $('#m_fechamento').removeClass("erroInput");
                $('#erroFechamento').removeClass("erroDiv");
                $('#erroFechamento').hide();
            });

            $('#modal_dt_contabil').change(function () {
                $('#modal_dt_contabil').removeClass("erroInput");
                $('#erroDtContabil').removeClass("erroDiv");
                $('#erroDtContabil').hide();
            });

            $('#m_situacao').change(function () {
                $('#m_situacao').removeClass("erroInput");
                $('#erroSituacao').removeClass("erroDiv");
                $('#erroSituacao').hide();
            });

            $('#modalUpdate').on('shown.bs.modal', function () {
                $(this).find('.modal-dialog').css({
                    width: '100%',
                    height: 'auto',
                    'max-height': '100%',
                    'padding-right': 0,
                    'padding-left': 0
                });

                $(this).css({
                    'padding-right': 0,
                    'padding-left': 0
                });

            });

            $('#modalUpdateSitFechado').on('shown.bs.modal', function () {
                $(this).find('.modal-dialog').css({
                    width: '100%',
                    height: 'auto',
                    'max-height': '100%',
                    'padding-right': 0
                });

                $(this).css({
                    'padding-right': 0
                });

            });



            $("#modal_dt_contabil").keypress(function (event) { event.preventDefault(); });

            $('.menuContabilizacao .sidebar-submenu').addClass("active");
            $('.menuContabilizacao .sidebar-submenu').toggle();

        </script>
