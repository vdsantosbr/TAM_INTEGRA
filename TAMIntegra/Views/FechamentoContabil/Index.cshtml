                                            }
                                        }
                                        else {
                                            $('#gridCambio tbody').append('<td style="padding: 0 10px 0 10px;">' + '' + '</td>');
                                        }
                                        $('#gridCambio tbody').append('<td style="text-align:center">' + response.Cambio[x].Id_integracao + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:center">' + ftm_dt_cambio + '</td>');
                                        $('#gridCambio tbody').append('<td>' + response.Cambio[x].Nome + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:center">' + ftm_dt_transacao_cambio + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:right">' + response.Cambio[x].Qtd_registros.toLocaleString() + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:right">' + Vc_Efetiva_Ganho + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:right">' + Vc_Efetiva_Perda + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:right">' + Vc_Nao_Efetiva_Ganho + '</td>');
                                        $('#gridCambio tbody').append('<td style="text-align:right">' + Vc_Nao_Efetiva_Perda + '</td>');
                                        $('#gridCambio tbody').append('</tr>');
                                    }
                                }
                            }


                            if (response.Resultado[0].Exibir_Salvar == 'Fechado') {
                                $('#btnSalvar').hide()
                            }
                            else {
                                $('#btnSalvar').show()
                            }


                                $('#modalUpdate').modal({
                                    show: true,
                                    closeOnEscape: true,
                                    backdrop: 'static',
