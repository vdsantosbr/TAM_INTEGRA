                                        $('#gridCambio tbody').append('<td style="background-color:#f2f2f2;text-align:right">' + Vc_Nao_Efetiva_Ganho + '</td>');
                                        $('#gridCambio tbody').append('<td style="background-color:#f2f2f2;text-align:right">' + Vc_Nao_Efetiva_Perda + '</td>');
                                        $('#gridCambio tbody').append('</tr>');
                                    }
                                    else {
                                        $('#gridCambio tbody').append('<tr>');
                                        if (response.Cambio[x].Ativar_RadioBox == "S") {
                                            if (response.Cambio[x].Valor_RadioBox == "true") {
                                                $('#gridCambio tbody').append('<td style="padding: 0 10px 0 10px;">' + '<input type="radio" name="ckCambio" value="' + response.Cambio[x].Id_integracao + '" checked/>' + '</td>');
                                            }
                                            else {
                                                $('#gridCambio tbody').append('<td style="padding: 0 10px 0 10px;">' + '<input type="radio" name="ckCambio" value="' + response.Cambio[x].Id_integracao + '"/>' + '</td>');
